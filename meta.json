{
  "$schema": "https://dl.viam.dev/module.schema.json",
  "module_id": "hipsterbrown:pi-hailo-ml",
  "visibility": "public",
  "url": "https://github.com/hipsterbrown/viam-pi-hailo-ml",
  "description": "Modular mlmodel service: hailo-rt",
  "build": {
    "build": "mise run package",
    "path": "module.tar.gz",
    "arch": [
      "linux/arm64"
    ]
  },
  "models": [
    {
      "api": "rdk:service:mlmodel",
      "model": "hipsterbrown:pi-hailo-ml:hailo-rt"
    }
  ],
  "entrypoint": "./run.sh",
  "first_run": "./setup.sh"
}
